{
  "expo": {
    "name": "frontend-spidey",
    "slug": "frontend-spidey",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
  "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.spidey.sense",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera within the embedded web experience to help guide navigation.",
        "NSMicrophoneUsageDescription": "This app plays audio guidance and may capture audio for voice features.",
        "NSAllowsArbitraryLoads": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "scheme": "spideysense",
    "experiments": {
      "tsconfigPaths": true
    }
  }
}
